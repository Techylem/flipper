{
  "name": "flipper",
  "appId": "flipper.rw",
  "version": "1.6.45",
  "description": "Flipper business app for business",
  "homepage": "https://flipper.yegobox.com/",
  "productName": "flipper",
  "author": {
    "name": "yegobox",
    "email": "flipper@yegobox.com"
  },
  "keywords": [
    "POS",
    "Inventory",
    "CRM",
    "Yegobox",
    "Flipper"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build -p  always --linux snap -c.snap.publish=github",
    "electron:windows": "npm run build:prod && npx electron-builder build --ia32 -p always --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build -p always  --mac",
    "electron:build": "npm run build:prod && electron-builder build",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "build-vendors": "ng build flippervendors --watch",
    "build-dialog": "ng build flipperdialog --watch",
    "build-event": "ng build flipperevent --watch",
    "build-color": "ng build flippercolor --watch",
    "build-component": "ng build flippercomponents --watch",
    "build-dashboard": "ng build flipperdashboard --watch",
    "build-font": "ng build flipperfont --watch",
    "build-stock": "ng build flipperinventory --watch",
    "build-pos": "ng build flipperpos --watch ",
    "build-db": "ng build flipperofflinedatabase --watch",
    "build-setting": "ng build flippersettings --watch",
    "build-upload": "ng build flipperfileupload --watch",
    "build-menu": "ng build flippermenu --watch",
    "build-card": "ng build paymentcard --watch",
    "buildAll": " ng build flippercolor && ng build flipperevent  && ng build flipperfont && ng build flipperfileupload   && ng build paymentcard  && ng build flippercomponents && ng build flippervendors     && ng build flipperdialog   && ng build flipperpos",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "config": {
    "ghooks": {
      "pre-commit": "",
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  },
  "dependencies": {
