{
  "name": "flipper",
  "version": "0.5.3",
  "description": "Flipper is software that is used in different business like (super market, pharmacies, industries, hotels etc..) and it is developed by team @enexus to manage inventory and sales",
  "homepage": "https://flipper.rw",
  "repository": {
    "type": "git",
    "url": "https://github.com/StreamUpBox/flipper.git"
  },
  "author": {
    "name": "Enexus Team",
    "email": "flipper@yegobox.com"
  },
  "keywords": [
    "POS",
    "Pharmacies",
    "Hospitals",
    "Management",
    "Business",
    "Stock"
  ],
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start:all": "npm-run-all -p ng:serve electron:serve",
    "start": "npm-run-all -p ng:serve:web electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "build:web": "ng build --source-map=false --prod --deploy-url=flipper/",
    "build:electron": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildMemory": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --source-map=false --prod --source-map --common-chunk=false --deploy-url=domains/",
    "postinstall:web": "node postinstall-web.js",
    "postinstall:electron": "node postinstall.js",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "ng:serve": "npm run postinstall:web && ng serve --aot --proxy-config proxy.conf.json --prod",
    "ng:serve:web": "npm run postinstall:web && ng serve -o --aot --proxy-config proxy.conf.json --prod",
    "ng:memory": "npm run postinstall:web &&  node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve -o --aot --proxy-config proxy.conf.json --prod --port=8080",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run postinstall:electron && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build -p  always --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --x64 --ia32 -p always --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build -p always  --mac",
    "test": "npm run postinstall:web && ng test --watch",
    "test:travis": "npm run postinstall:web && ng test  --browsers=ChromeHeadless",
    "release": "standard-version",
    "mocha": "mocha",
    "alpha": "npm run release -- --prerelease alpha",
    "major": "npm run release -- --release-as major",
    "minor": "npm run release -- --release-as minor",
    "patch": "npm run release -- --release-as patch"
  },
  "config": {
    "ghooks": {
      "pre-commit": "ng lint --fix",
      "post-merge": "npm install"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/cdk": "^8.2.0",
    "@angular/cli": "^8.3.8",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.2.5",
    "@angular/material": "^8.2.0",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "~8.2.5",
    "@ngrx/effects": "^8.2.0",
    "@ngrx/store": "^8.2.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@ngxs/store": "^3.5.0",
    "@types/electron": "^1.6.10",
    "bootstrap": "^4.3.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.0",
    "custom-electron-titlebar": "^3.1.0",
    "deepmerge": "^4.0.0",
    "dot-object": "^1.9.0",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^2.2.17",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.1.2",
    "hammerjs": "^2.0.8",
    "ngx-color-picker": "^7.5.0",
    "ngx-electron": "^2.1.1",
    "ngx-model": "^6.0.2",
    "ngx-webstorage": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "pusher-js": "^4.4.0",
    "raven-js": "^3.27.2",
    "rxjs": "~6.4.0",
    "rxjs-compat": "^6.5.2",
    "singleton-js": "^1.0.0",
    "tslib": "^1.10.0",
    "wait-on": "^3.3.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.4",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@ngx-translate/core": "^10.0.1",
    "@sentry/browser": "^4.6.6",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "cheerio": "^1.0.0-rc.3",
    "codelyzer": "^5.0.0",
    "electron": "^3.1.13",
    "electron-builder": "^20.44.4",
    "electron-rebuild": "^1.8.6",
    "electron-reload": "^1.2.2",
    "ghooks": "^2.0.4",
    "gulp": "^4.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-filter": "^5.1.0",
    "gulp-rename": "^1.4.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-svgmin": "^2.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "^3.4.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npx": "^10.2.0",
    "perfect-scrollbar": "^1.4.0",
    "popper.js": "^1.15.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "release": "^6.1.0"
  }
}
